{
    "name": "vue-minesweeper",
    "version": "0.1.2",
    "description": "Minesweeper plugin for vuejs",
    "license": "MIT",
    "author": "disjfa <disjfa@disjfa.nl>",
    "homepage": "https://disjfa.github.io/vue-minesweeper/",
    "repository": {
        "type": "git",
        "url": "https://github.com/disjfa/vue-minesweeper"
    },
    "bugs": {
        "url": "https://github.com/disjfa/vue-minesweeper/issues"
    },
    "scripts": {
        "build": "vue-cli-service build --target lib --name VueMinesweeper src/index.js",
        "build:demo": "vue-cli-service build ./demo/main",
        "contributors:add": "all-contributors add",
        "contributors:generate": "all-contributors generate",
        "deploy": "npm run build:demo && gh-pages -d dist",
        "lint": "vue-cli-service lint",
        "serve": "vue-cli-service serve ./demo/main",
        "test:unit": "vue-cli-service test:unit"
    },
    "peerDependencies": {
        "vue": "^2.6.6"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.7.0",
        "@vue/cli-plugin-eslint": "^3.7.0",
        "@vue/cli-plugin-unit-jest": "^3.7.0",
        "@vue/cli-service": "^3.7.0",
        "@vue/eslint-config-airbnb": "^4.0.0",
        "@vue/test-utils": "1.0.0-beta.29",
        "all-contributors-cli": "^6.4.0",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^23.6.0",
        "core-js": "^2.6.5",
        "eslint": "^5.8.0",
        "eslint-plugin-vue": "^5.0.0",
        "gh-pages": "^2.0.1",
        "node-sass": "^4.12.0",
        "sass-loader": "^7.1.0",
        "vue": "^2.6.6",
        "vue-router": "^3.0.6",
        "vue-template-compiler": "^2.5.21"
    },
    "main": "dist/VueMinesweeper.umd.js",
    "browser": "dist/VueMinesweeper.common.js",
    "unpkg": "dist/VueMinesweeper.umd.min.js",
    "jsDelivr": "dist/VueMinesweeper.umd.min.js",
    "files": [
        "dist",
        "src"
